{
	"name": "cadviewer",
	"description": "A Cadviewer Nextcloud app",
	"version": "1.0.0",
	"contributors": [
		"Kevin WAMBA <kevin.wamba@kevmax.com>"
	],
	"private": true,
	"scripts": {
		"build": "NODE_ENV=production webpack --progress --config webpack.js",
		"dev": "NODE_ENV=development webpack --progress --config webpack.js && replace-in-file  --configFile=replace-in-file-config.js",
		"watch": "NODE_ENV=development webpack --progress --watch --config webpack.js",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"stylelint": "stylelint src",
		"stylelint:fix": "stylelint src --fix"
	},
	"dependencies": {
		"@nextcloud/l10n": "^1.4.1",
		"@nextcloud/router": "^2.0.0",
		"@nextcloud/vue": "^7.0.0",
		"cadviewer": "^7.2.5",
		"core-js": "^3.25.0",
		"popper.js": "^1.16.1",
		"vue": "^2.7.13",
		"vue-template-compiler": "^2.7.13",
		"xml-loader": "^1.2.1"
	},
	"devDependencies": {
		"@nextcloud/babel-config": "^1.0.0",
		"@nextcloud/browserslist-config": "^2.3.0",
		"@nextcloud/eslint-config": "^8.0.0",
		"@nextcloud/stylelint-config": "^2.1.2",
		"@nextcloud/webpack-vue-config": "^5.1.0",
		"@relative-ci/agent": "^4.1.1",
		"@vue/cli-plugin-babel": "^4.5.19",
		"@vue/cli-service": "^4.5.19",
		"@vue/test-utils": "^1.3.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.3",
		"css-loader": "^6.5.1",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"jest": "^29.1.2",
		"jest-serializer-vue": "^2.0.2",
		"node-polyfill-webpack-plugin": "^1.1.4",
		"replace-in-file": "^6.3.5",
		"sass": "^1.47.0",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"vue-jest": "^3.0.7",
		"vue-loader": "^15.9.8",
		"vue-style-loader": "^4.1.0",
		"webpack": "^5.66.0",
